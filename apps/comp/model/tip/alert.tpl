	{{if flag=='fail'}}<img src="/baseop/nresource/pub-ui/images/dialog-fail.png" alt="" />{{/if}}
	<div class="inline-block" style="vertical-align: middle;">
		<p class="color-lightblue fs-28">{{title}}</p>
		<p class="mw-310 mgt-10">{{msg}}</p>
	</div>
</div>